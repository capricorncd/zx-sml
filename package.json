{
  "name": "zx-sml",
  "version": "0.10.0",
  "description": "Some static method library and documentation generator",
  "type": "module",
  "main": "./libs/utils/index.js",
  "types": "./libs/utils/types.d.ts",
  "exports": {
    ".": {
      "import": "./libs/utils/index.js",
      "require": "./libs/utils/index.js",
      "types": "./libs/utils/types.d.ts"
    },
    "./docgen": {
      "import": "./libs/docgen/index.mjs",
      "require": "./dist/docgen/index.mjs",
      "types": "./libs/docgen/types.d.ts"
    },
    "./nodejs": {
      "require": "./dist/docgen/index.mjs",
      "types": "./libs/docgen/types.d.ts"
    }
  },
  "files": [
    "libs",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "zx-libs",
    "static method library",
    "format",
    "method",
    "typescript",
    "vite",
    "vitest",
    "documentation generator"
  ],
  "dependencies": {
    "@zx-libs/docgen": "^0.0.2",
    "@zx-libs/utils": "^0.0.5"
  },
  "devDependencies": {
    "inquirer": "^9.2.15"
  },
  "engines": {
    "node": ">=16.13.1",
    "npm": ">=8.1.2"
  },
  "homepage": "https://github.com/capricorncd/zx-libs/tree/main/libs/utils",
  "repository": {
    "url": "git+https://github.com/capricorncd/zx-sml.git"
  },
  "author": "Capricorncd <capricorncd@qq.com>",
  "license": "MIT"
}
